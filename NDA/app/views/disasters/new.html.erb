<%= form_for :disaster, url: disasters_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %><br>
  </p>
  <p>
    <%= f.label :preparation %><br>
    <%= f.text_area :description %><br>
  </p>
  <p>
    <%= f.label :photo_url %><br>
    <%= f.text_area :photo_url %>
  </p>


  <p>
    <%= f.submit %>
  </p>
<% end %>
